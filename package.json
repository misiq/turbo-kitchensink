{
	"name": "turbo-kitchensink",
	"private": true,
	"scripts": {
		"build": "bun run packages/build-ui/src/cli.ts",
		"build:turbo": "turbo build",
		"dev": "turbo run dev",
		"lint": "turbo run lint",
		"format": "prettier --write \"**/*.{ts,tsx,md}\"",
		"check-types": "turbo run check-types"
	},
	"devDependencies": {
		"prettier": "^3.7.4",
		"turbo": "^2.8.3",
		"typescript": "5.9.2"
	},
	"dependencies": {
		"@misq/build-ui": "workspace:*"
	},
	"engines": {
		"node": ">=18"
	},
	"packageManager": "bun@1.2.23",
	"workspaces": [
		"apps/*",
		"packages/*"
	]
}
